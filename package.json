{
  "name": "orbit-strategies-unofficial",
  "version": "0.1.3",
  "description": "Experimental strategies for Orbit.js",
  "contributors": [
    "Paul Chavard <github@paul.chavard.net>"
  ],
  "keywords": [
    "orbit",
    "orbit.js",
    "data",
    "synchronization"
  ],
  "repository": "https://github.com/tchak/orbit-strategies-unofficial",
  "license": "MIT",
  "main": "dist/commonjs/es2017/index.js",
  "module": "dist/modules/es2017/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && BROCCOLI_ENV=dist broccoli build dist",
    "build:tests": "rm -rf tests && BROCCOLI_ENV=tests broccoli build tests",
    "clean": "git clean -x -f",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run clean",
    "test": "testem ci",
    "lint:ts": "eslint . --ext .ts",
    "release": "release-it"
  },
  "dependencies": {
    "@orbit/coordinator": "^0.17.0-beta.1",
    "@orbit/core": "^0.17.0-beta.1",
    "@orbit/data": "^0.17.0-beta.1",
    "@orbit/record-cache": "^0.17.0-beta.1",
    "@orbit/utils": "^0.17.0-beta.1"
  },
  "devDependencies": {
    "@glimmer/build": "^0.10.3",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "release-it": "^12.4.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "github": {
      "release": false
    }
  }
}
